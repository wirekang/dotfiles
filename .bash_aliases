alias la='ls -lA'

alias ggs="git status"
alias gga="git add"
alias ggap="git add . -p"
alias ggc="git commit"
alias ggca="git commit --amend"
alias ggcm="git commit -m"
alias ggm="git merge"
alias ggp="git push"
alias ggpa="git push --all"
alias ggps="git push --set-upstream origin"
alias ggpf="git pull --ff-only"
alias ggb="git branch"
alias ggbd="git branch -d"
alias ggd="git diff"
alias ggds="git diff --staged"
alias ggco="git checkout"
alias ggfa="git fetch --all"
alias ggr="git restore"
alias ggrs="git restore --staged"
alias ggl="git log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all"

alias ports='sudo netstat -antp | grep LISTEN'
